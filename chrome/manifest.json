{
	"manifest_version": 3,
	"name": "Listing Retrieval",
	"version": "2024.11.23.1",
	"description": "Retrieve listing from a website",
	"permissions": [
	  "storage",
	  "tabs",
	  "activeTab",
	  "scripting",
	  "downloads"
	],
	"host_permissions": [
	  "https://www.mercari.com/*",
	  "https://www.ebay.com/*",
	  "https://www.etsy.com/*",
	  "https://www.poshmark.com/*",
	  "https://poshmark.com/*",
	  "https://shopgoodwill.com/*",
	  "https://vi.vipr.ebaydesc.com/*",
	  "https://poshmark.com/closet/*",
	  "https://district.net/*"
	],
	"action": {
	  "default_popup": "pages/popup.html"
	},
	"background": {
	  "service_worker": "background.js",
	  "type": "module"
	},
	"options_page": "options.html",
	"icons": {
	  "128": "ListFlow_icon.png"
	}
  }
